{
  "name": "@danmasta/lo",
  "private": true,
  "version": "0.0.7",
  "author": "Daniel Smith <d@danmasta.net>",
  "description": "Lightweight, modern utility library for node, browser, and quickjs",
  "license": "Apache-2.0",
  "keywords": [
    "util",
    "stdlib",
    "node",
    "browser",
    "quickjs"
  ],
  "main": "index.js",
  "browser": "browser.js",
  "type": "module",
  "files": [
    "browser.js",
    "index.js",
    "qjs.js",
    "lib/**",
    "polyfill/**",
    "types/**"
  ],
  "exports": {
    ".": {
      "browser": "./browser.js",
      "qjs": "./qjs.js",
      "default": "./index.js"
    },
    "./*": "./lib/*.js",
    "./browser": "./browser.js",
    "./polyfill/*": "./polyfill/base/*.js",
    "./polyfill/qjs/*": "./polyfill/qjs/*.js",
    "./qjs": "./qjs.js"
  },
  "imports": {
    "#lib/*": "./lib/*.js",
    "#polyfill/*": "./polyfill/base/*.js",
    "#polyfill/qjs/*": "./polyfill/qjs/*.js"
  },
  "scripts": {
    "preversion": "make test status"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danmasta/lo.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^5.2.1",
    "eslint": "^9.32.0",
    "mocha": "^11.7.1"
  }
}
